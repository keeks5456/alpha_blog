
<table>
  <thead>
    <tr>
      <th>All Articles</th>
      <th>Description</th>
      <th>Actions</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
  <% @articles.each do |article|%>
    <tr>
    <td><%= article.title %></td>
    <td><%= article.description %></td>
    <td><%= link_to "Show", article %></td>
    </tr>
    <%end%>
  </tbody>
</table>



